{
  "name": "react-validate-framework",
  "version": "0.3.2",
  "description": "React validation framework.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src config --fix",
    "start": "better-npm-run start",
    "deploy": "npm run clean && better-npm-run prod"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node config/server.dev.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "prod": {
      "command": "babel src --out-dir lib --source-maps --extensions .js,.jsx --copy-files",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MinJieLiu/react-validate-framework.git"
  },
  "keywords": [
    "react-form",
    "react-validation",
    "react-validate",
    "react-validate-framework"
  ],
  "author": "LMY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MinJieLiu/react-validate-framework/issues"
  },
  "homepage": "https://github.com/MinJieLiu/react-validate-framework#readme",
  "dependencies": {
    "validate-framework-utils": "0.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.13",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "debug": "^2.5.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.13.2"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x",
    "react-dom": "0.14.x || 15.x"
  }
}
